{
  "name": "ignore_paths",
  "tree": {
    "$className": "DataModel",
    "Workspace": {
      "$properties": {
        "EditorLiveScripting": {
          "Enum": 0
        },
        "SignalBehavior": "Deferred",
        "StreamOutBehavior": "Opportunistic",
        "StreamingEnabled": true,
        "StreamingIntegrityMode": "PauseOutsideLoadedArea"
      },
      "$path": "src"
    }
  },
  "syncbackRules": {
    "ignorePaths": [
      "src/*.rbxm",
      "src/folder_name/**"
    ]
  }
}